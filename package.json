  "author": "Mateo Nieto Hoyos <matheo.developer@gmail.com>",
  "license": "GPL3",
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "dev": "nodemon src/index.ts --exec ts-node",
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist ./coverage",
    "update": "yarn upgrade-interactive --latest",
    "dev:test": "npm run build && jest ./dist/ --watch",
    "test:report": "npm run build && jest .,/dist/ --coverage",
    "test": "npm run build && jest ./dist/",
    "makemigrations": "npx ts-node ./node_modules/.bin/typeorm migration:generate -n $",
    "migrate": "npx ts-node ./node_modules/.bin/typeorm migration:run",
    "revermigrate": "npx ts-node ./node_modules/.bin/typeorm migration:revert"
  },
  "dependencies": {
    "@hapi/boom": "9.1.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.19.0",
    "cookie-parse": "0.4.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "joi": "17.4.0",
    "jsonwebtoken": "8.5.1",
    "moment": "2.29.1",
    "mysql": "2.18.1",
    "passport": "0.4.1",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0",
    "randomatic": "3.1.1",
    "reflect-metadata": "0.1.13",
    "socket.io": "3.1.2",
    "typeorm": "0.2.31"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/jsonwebtoken": "8.5.0",
    "@types/morgan": "1.9.2",
    "@types/node": "14.14.31",
    "@types/passport": "1.0.6",
    "@types/passport-http": "0.3.8",
    "@types/passport-jwt": "3.0.5",
    "@types/randomatic": "3.1.0",
    "@types/supertest": "2.0.10",
    "casual": "1.6.2",
    "jest": "26.6.3",
    "morgan": "1.10.0",
    "nodemon": "2.0.7",
    "rimraf": "3.0.2",
    "supertest": "6.1.3",
    "ts-node": "9.1.1",
    "typescript": "4.2.2"
  }
}
